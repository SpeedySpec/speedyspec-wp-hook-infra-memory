{
    "name": "speedyspec/speedyspec-wp-hook-infra-memory",
    "version": "0.0.3",
    "description": "SpeedySpec Rewrite of WordPress - Hook API Infrastructure Memory Package ",
    "type": "library",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=8.4",
        "speedyspec/speedyspec-wp-hook-domain": "^1"
    },
    "require-dev": {
        "pestphp/pest": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "SpeedySpec\\WP\\Hook\\Infra\\Memory\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpeedySpec\\WP\\Hook\\Infra\\Memory\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
